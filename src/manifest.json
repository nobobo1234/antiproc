{
    "manifest_version": 2,
    "version": "0.1",
    "name": "AntiProc",
    "options_page": "options.html",
    "background": {
        "scripts": ["background_script.js"]
    },
    "permissions": [
        "storage", "tabs", "webNavigation"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "icons": {
        "128": "imgs/logo.png",
        "16": "imgs/logo.png",
        "48": "imgs/logo.png"
    }
}